<h1><%= title %></h1>

<nav id="shows">
	<ul>
	<% for ( var i = 0; i < files.length; i++ ) { %>
		<li>
			<a href="<%= files[ i ] %>/"><%= files[ i ] %>
				<% if ( shows[ files[ i ]] && typeof shows[ files[ i ]].oflc === 'object' && shows[ files[ i ]].oflc.classification.length > 0 ) { %>
					<img class="oflc" src="/img/oflc/<%= shows[ files[ i ]].oflc.classification %>.png" alt="<%= shows[ files[ i ]].oflc.classification %>" />
				<% } %>
			</a>
		</li>
	<% } %>
	</ul>
</nav>
